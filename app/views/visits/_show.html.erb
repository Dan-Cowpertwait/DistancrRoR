<% @visits.each do |visit| %>

<div class="container">
  <div class="card-deck mb-3 text-center">
    <div class="card mb-4 shadow-sm">
      <div class="card-header">
        <h4 class="my-0 font-weight-normal"><%= visit.place.name %> </h4>
      </div>
      <div class="card-body">
        <ul class="list-unstyled mt-3 mb-4">
          <li><%= visit.created_at %></li>
        </ul>
        <% if @individual.profile_pic.attached?%>
            <%= image_tag(@individual.profile_pic, class:"img-fluid rounded-circle") %>
        <% else%>
            <img class="my-img" style="width: 250px; height: 250px;" src=<%=asset_path("QuestionMark.png")%>>
        <% end %>
      </div>
    </div>
  </div>
<% end %>